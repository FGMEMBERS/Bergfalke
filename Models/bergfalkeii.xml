        <entry>
            <ind>0.2</ind>
            <dep>-1</dep>
        </entry>
        <entry>
            <ind>1</ind>
            <dep>-80</dep>
        </entry>
    </interpolation>
    <axis>
        <x1-m>-2.89859</x1-m>
        <y1-m>0.28915</y1-m>
        <z1-m>-0.06667</z1-m>
        <x2-m>-2.10143</x2-m>
        <y2-m>0.28915</y2-m>
        <z2-m>0.02292</z2-m>
    </axis>
 </animation>
 
    <!--Small windows-->
    <animation>
        <type>slider</type>
        <object-name>windowF</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-adjust</command>
                <property>sim/model/door-positions/frontwin/position-norm</property>
