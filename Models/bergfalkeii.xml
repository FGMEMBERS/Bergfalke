 <!--Canopy-->
  <animation>
    <type>pick</type>
     <object-name>canopyframe</object-name><!--
     <object-name>canopy.glass.int</object-name>
     <object-name>canopy.glass.ext</object-name>-->
    <visible>true</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>nasal</command>
        <script>doors.canopy.toggle();</script>
      </binding>
    </action>
  </animation>
 
 <animation>
    <type>rotate</type>
    <object-name>canopyframe</object-name>
    <object-name>canopy.glass.int</object-name>
